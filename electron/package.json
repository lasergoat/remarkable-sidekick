{
  "name": "remarkable-toolbox",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "cross-env NODE_ENV=development webpack --watch --progress --color",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "cross-env NODE_ENV=production webpack && electron-builder --dir",
    "dist": "cross-env NODE_ENV=production webpack && electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "",
  "license": "ISC",
  "build": {
    "appId": "org.rs.RemarkableSidekick",
    "productName": "Remarkable Sidekick",
    "extends": null,
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "assets/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    }
  },
  "dependencies": {
    "axios": "^0.21.1",
    "electron-log": "^4.4.1",
    "history": "^5.0.0",
    "jwt-decode": "^3.1.2",
    "keytar": "^7.7.0",
    "node-ssh": "^12.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "recoil": "^0.3.1",
    "sharp": "^0.28.3",
    "ssh2": "^1.2.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@headlessui/react": "^1.4.0",
    "@tailwindcss/forms": "^0.3.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.2",
    "@types/ssh2": "^0.5.47",
    "@types/styled-components": "^5.1.11",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.2.0",
    "electron": "^13.1.9",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "node-loader": "^2.0.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "styled-components": "^5.3.0",
    "tailwindcss": "^2.2.7",
    "ts-loader": "^6.1.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "webpack-merge": "^5.8.0"
  }
}
